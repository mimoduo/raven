{
  "name": "raven",
  "version": "0.6.0",
  "description": "A refined and flexible static blog generator",
  "keywords": [
    "blog",
    "boilerplate",
    "gulp",
    "pug",
    "sass",
    "markdown"
  ],
  "author": "Bryan Stoner",
  "email": "mimoduo@gmail.com",
  "link": "http://mimoduo.surge.sh/",
  "license": "MIT",
  "data": {
    "site": {
      "name": "Raven",
      "description": "A refined and flexible static blog generator",
      "url": "http://raven.surge.sh/"
    },
    "fonts": [
      {
        "family": "Playfair+Display",
        "weights": "400"
      },
      {
        "family": "Oxygen",
        "weights": "300,400,700"
      }
    ],
    "analytics": {
      "include": true,
      "UA": "UA-66664949-3"
    },
    "disqus": {
      "include": true,
      "shortname": "raven-blog"
    },
    "static": "output",
    "theme": "themes/feather",
    "templates": "themes/feather",
    "pager": 8,
    "links": {
      "social": [
        {
          "name": "Github",
          "url": "https://github.com/mimoduo"
        },
        {
          "name": "Codepen",
          "url": "https://codepen.io/mimoduo/"
        }
      ]
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "browser-sync": "^2.18.13",
    "del": "^3.0.0",
    "feed": "^1.1.0",
    "front-matter": "^2.2.0",
    "fs": "0.0.1-security",
    "globby": "^6.1.0",
    "gulp": "^3.9.1",
    "gulp-front-matter": "^1.3.0",
    "gulp-postcss": "^7.0.0",
    "gulp-pug": "^3.3.0",
    "gulp-sass": "^3.1.0",
    "gulp-sass-glob": "^1.0.8",
    "gulp-surge": "^0.1.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "highlight.js": "^9.12.0",
    "hljs": "^6.2.3",
    "markdown-it": "^12.3.2",
    "markdown-it-named-headers": "0.0.4",
    "pug": "^2.0.0-rc.4",
    "require-reload": "^0.2.2"
  }
}
